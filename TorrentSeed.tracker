<trackerinfo
	type="ts"
	shortName="TS"
	longName="TorrentSeed"
	siteName="torrentseed.org"
	checkRegisteredTorrent="false">

	<settings>
		<cookie_description/>
		<cookie/>
	</settings>

	<servers>
		<server
			serverNames="irc.p2p-network.net,irc.p2p-irc.net"
			channelNames="#torrentseed"
			announcerNames="TS-Announce"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--[New Torrent Uploaded] [Movies/DVDR] Law.Abiding.Citizen.DVDR-BOW .:. Size: 4.42 GB .:. Pre Time: 22m 36s .:. URL: http://torrentseed.org/details.php?id=12161&hit=1-->
				<regex value="^\[New Torrent Uploaded\]\s*\[([^\]]*)\](.*)\.:\.\s*Size:(.*)\.:\.\s*Pre Time:(.*)\.:\.\s*URL: https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)"/>
				<vars>
					<var name="category"/>
					<var name="torrentName"/>
					<var name="torrentSize"/>
					<var name="preTime"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
			<extract>
				<!--[New Torrent Uploaded] [TV/x264] Doctor.Who.2005.S05E09.720p.HDTV.x264-BiA  .:. Size: 1.12 GB .:. URL: http://torrentseed.org/details.php?id=16756&hit=1-->
				<regex value="^\[New Torrent Uploaded\]\s*\[([^\]]*)\](.*)\.:\.\s*Size:(.*)\.:\.\s*URL: https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)"/>
				<vars>
					<var name="category"/>
					<var name="torrentName"/>
					<var name="torrentSize"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="download.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="torrentName"/>
				<string value=".torrent"/>
			</var>
			<http name="Cookie">
				<var name="cookie"/>
			</http>
		</linematched>
		<ignore>
			<regex value="^User: "/>
			<regex value="^\w+ \(.*\) last seen "/>
			<regex value="^test "/>
			<regex value="No such user, please try again"/>
		</ignore>
	</parseinfo>
</trackerinfo>
