<trackerinfo
	type="ddt"
	shortName="DDT"
	longName="DIDIDave.com"
	siteName="www.dididave.com"
	checkRegisteredTorrent="false">

	<settings>
		<description text="Go to http://www.dididave.com/getrss.php to get the RSS feed link. Paste it (Ctrl+V) into the text box below to automatically extract passkey."/>
		<passkey tooltiptext="The passkey in your DIDIDave.com RSS feed link."/>
	</settings>

	<servers>
		<server
			serverNames="irc.dididave.com"
			channelNames="#siteannounce"
			announcerNames="didibot2"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--New Torrent Uploaded: Name: The.Soup.Presents.Anarchy.in.the.UK.PDTV.XviD-MOMENTUM Category: TV/Episodes Size: 175.10 MB URL: http://www.dididave.com/details.php?id=20356 Time After Pre: N/A-->
				<regex value="^New Torrent Uploaded: Name:(.*)Category:(.*)Size:(.*)URL: \s*https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)\s* Time After Pre:(.*)"/>
				<vars>
					<var name="torrentName"/>
					<var name="category"/>
					<var name="torrentSize"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
					<var name="preTime"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="rssdownload.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<var name="passkey"/>
				<string value="/"/>
				<varenc name="torrentName"/>
				<string value=".torrent"/>
			</var>
		</linematched>
		<ignore>
			<regex value="^New Torrent Uploaded:" expected="false"/>
		</ignore>
	</parseinfo>
</trackerinfo>
